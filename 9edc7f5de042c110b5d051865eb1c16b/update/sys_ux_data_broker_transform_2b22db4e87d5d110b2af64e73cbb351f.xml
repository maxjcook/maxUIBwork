        <script><![CDATA[function transform(input){
	var deptArr = [];

	var dGR = new GlideRecord('cmn_department');
	dGR.query();
	while(dGR.next()){
		var depts = {};
		depts.id = dGR.getValue('sys_id');
		depts.label = dGR.getValue('name');
		deptArr.push(depts);
	}
	return deptArr;
}]]></script>
