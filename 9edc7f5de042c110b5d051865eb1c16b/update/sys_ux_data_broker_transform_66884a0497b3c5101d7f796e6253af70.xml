        <script><![CDATA[function transform(input){

	if(input){
		var name = input.name.toString();
		var desc = input.description.toString();
		var add = input.address.toString();
		var invt = input.inventory.toString();
		var dept = input.department.toString();
		gs.info('SL name: '  + name);
		gs.info('SL desc: ' + desc);
		gs.info('SL add: ' + add);
		gs.info('SL invt: ' + invt);
		gs.info('SL dept: ' + dept);



		// use the sys id from each dropdown to look up the 
		// display values to populate into the new record
		var nSL = new GlideRecord("x_snc_trackit_stock_location");

		nSL.initialize();
		nSL.setValue('name',name);
		nSL.setValue('description',desc);
		nSL.setValue('address',add);
		nSL.setValue('inventory_manager',invt);
		nSL.setValue('department',dept);
		nSL.insert();	
		gs.info('MJC new stock location: ' + nSL.getValue('sys_id'));
	}

	var msg = nSL.getValue('sys_id');
	return msg;
}]]></script>
